{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
          "bodyParser": false
        }
      }
    ],
    "routes": [
      { "src": "/api/interactions", "dest": "/api/index.js" },
      { "src": "/.*", "status": 404, "dest": "/api/index.html" }
    ]
  }